# Rename this file to `.env` and fill in your own values.
# PostgreSQL database connection string
DATABASE_URL=postgresql://USER:PASSWORD@localhost:5432/movie_tracker

# OMDb API key (get one for free at https://www.omdbapi.com/apikey.aspx)
OMDB_API_KEY=your_omdb_api_key_here

# Secret used to sign JWTs for authentication
JWT_SECRET=your_jwt_secret_here

# If running locally, set NODE_ENV=development
NODE_ENV=development